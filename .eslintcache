 method: 'delete',\n    });\n    if (response.status === 200) {\n      const { customersReducer } = getState();\n      const customersCopy = [...customersReducer.customersData];\n      dispatch(onFetchCustomerSucced(customersCopy.filter((cus) => cus._id !== customerId)));\n    }\n  } catch (e) {\n    console.log('Unable to delete', e);\n  }\n};\n\nexport const updateCustomerFetch = (updatedItem) => async (dispatch, getState) => {\n  try {\n    const response = await fetch(`http://localhost:3001/api/customers/${updatedItem._id}`, {\n      method: 'put',\n      headers: {\n        'Content-Type': 'application/json',\n      },\n      body: JSON.stringify(updatedItem),\n    });\n    const resJson = await response.json();\n    const { customersReducer } = getState();\n    const customersCopy = [...customersReducer.customersData];\n    const updatedArray = customersCopy.map((custom) => (custom._id === resJson.data._id\n      ? resJson.data : custom));\n    dispatch(onFetchCustomerSucced(updatedArray));\n  } catch (e) {\n    console.log('Unable to update', e);\n  }\n};\n","C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\Components\\Sections\\BuildingsSection\\validations.js",[],"C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\Components\\Sections\\BuildingsSection\\TextInput.js",[],"C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\Components\\Sections\\BoilerTypesSection\\validations.js",[],"C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\Components\\ContentTable\\ContentTable.js",[],"C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\Components\\TransitionModal\\Select\\Select.js",[],"C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\redux\\types\\customers.js",[],"C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\Components\\ListItem\\ListItem.js",[],"C:\\Users\\Ayom\\Desktop\\caldAR-react-app\\src\\redux\\reducers\\authReducer.js",[],{"ruleId":"213","replacedBy":"214"},{"ruleId":"215","replacedBy":"216"},{"ruleId":"217","replacedBy":"218"},{"ruleId":"219","replacedBy":"2